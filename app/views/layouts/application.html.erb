<!DOCTYPE html>
<html>
<head>
  <title>Blog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<h1>Wouter de Vos</h1>

<% flash.each do |type, message| %>
  <div class="flash <%= type %>">
    <p><%= message %></p>
  </div>
<% end %>

<%= yield %>

</body>
</html>
